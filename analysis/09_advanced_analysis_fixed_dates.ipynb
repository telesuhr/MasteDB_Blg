    "## 1. Term Structure Evolution with Fixed Date Axes"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate term structure metrics\n",
    "def calculate_term_structure_metrics(price_data):\n",
    "    \"\"\"Calculate various term structure metrics\"\"\"\n",
    "    metrics = pd.DataFrame(index=price_data.index)\n",
    "    \n",
    "    # Basic spreads\n",
    "    if 1 in price_data.columns and 3 in price_data.columns:\n",
    "        metrics['M1_M3_Spread'] = price_data[3] - price_data[1]\n",
    "        metrics['M1_M3_Ratio'] = price_data[3] / price_data[1]\n",
    "    \n",
    "    if 1 in price_data.columns and 6 in price_data.columns:\n",
    "        metrics['M1_M6_Spread'] = price_data[6] - price_data[1]\n",
    "        \n",
    "    if 1 in price_data.columns and 12 in price_data.columns:\n",
    "        metrics['M1_M12_Spread'] = price_data[12] - price_data[1]\n",
    "    \n",
    "    # Curve steepness\n",
    "    front_tenors = [col for col in price_data.columns if col <= 3]\n",
    "    back_tenors = [col for col in price_data.columns if col >= 9]\n",
    "    \n",
    "    if front_tenors and back_tenors:\n",
    "        front_avg = price_data[front_tenors].mean(axis=1)\n",
    "        back_avg = price_data[back_tenors].mean(axis=1)\n",
    "        metrics['Curve_Steepness'] = back_avg - front_avg\n",
    "    \n",
    "    return metrics.dropna(how='all')\n",
    "\n",
    "# Calculate metrics\n",
    "ts_metrics = calculate_term_structure_metrics(price_pivot)\n",
    "\n",
    "# Create visualization with proper date formatting\n",
    "fig = plt.figure(figsize=(18, 12))\n",
    "gs = GridSpec(3, 2, figure=fig, hspace=0.4, wspace=0.3)\n",
    "\n",
    "# 1. Key spreads over time\n",
    "ax1 = fig.add_subplot(gs[0, :])\n",
    "if 'M1_M3_Spread' in ts_metrics.columns:\n",
    "    ax1.plot(ts_metrics.index, ts_metrics['M1_M3_Spread'], \n",
    "            color=COLORS['primary'], linewidth=2.5, label='M1-M3')\n",
    "if 'M1_M6_Spread' in ts_metrics.columns:\n",
    "    ax1.plot(ts_metrics.index, ts_metrics['M1_M6_Spread'], \n",
    "            color=COLORS['secondary'], linewidth=2.5, label='M1-M6')\n",
    "if 'M1_M12_Spread' in ts_metrics.columns:\n",
    "    ax1.plot(ts_metrics.index, ts_metrics['M1_M12_Spread'], \n",
    "            color=COLORS['tertiary'], linewidth=2.5, label='M1-M12')\n",
    "\n",
    "ax1.axhline(y=0, color='black', linestyle='--', alpha=0.5)\n",
    "ax1.set_title('Calendar Spreads Evolution', fontsize=16, weight='bold')\n",
    "ax1.set_ylabel('Spread (USD/tonne)', fontsize=12)\n",
    "ax1.legend(fontsize=11)\n",
    "ax1.grid(True, alpha=0.3)\n",
    "\n",
    "# Format date axis\n",
    "format_date_axis(ax1, data_range_days)\n",
    "\n",
    "# 2. Front month price with trend\n",
    "ax2 = fig.add_subplot(gs[1, 0])\n",
    "if 1 in price_pivot.columns:\n",
    "    front_price = price_pivot[1].dropna()\n",
    "    ax2.plot(front_price.index, front_price, \n",
    "            color=COLORS['primary'], linewidth=2, label='Price')\n",
    "    \n",
    "    # Add moving averages\n",
    "    ma_20 = front_price.rolling(20).mean()\n",
    "    ma_50 = front_price.rolling(50).mean()\n",
    "    \n",
    "    ax2.plot(ma_20.index, ma_20, color=COLORS['secondary'], \n",
    "            linewidth=1.5, alpha=0.8, label='20-day MA')\n",
    "    ax2.plot(ma_50.index, ma_50, color=COLORS['tertiary'], \n",
    "            linewidth=1.5, alpha=0.8, label='50-day MA')\n",
    "    \n",
    "    ax2.set_title('Front Month Price Trend', fontsize=14, weight='bold')\n",
    "    ax2.set_ylabel('Price (USD/tonne)', fontsize=12)\n",
    "    ax2.legend(fontsize=10)\n",
    "    ax2.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax2, data_range_days)\n",
    "\n",
    "# 3. Volatility analysis\n",
    "ax3 = fig.add_subplot(gs[1, 1])\n",
    "if 1 in price_pivot.columns:\n",
    "    returns = price_pivot[1].pct_change().dropna() * 100\n",
    "    vol_20d = returns.rolling(20).std() * np.sqrt(252)\n",
    "    \n",
    "    ax3.plot(vol_20d.index, vol_20d, \n",
    "            color=COLORS['quaternary'], linewidth=2)\n",
    "    \n",
    "    # Add volatility zones\n",
    "    vol_mean = vol_20d.mean()\n",
    "    vol_std = vol_20d.std()\n",
    "    \n",
    "    ax3.axhline(vol_mean, color='green', linestyle='--', alpha=0.7, label='Average')\n",
    "    ax3.axhline(vol_mean + vol_std, color='orange', linestyle=':', alpha=0.7, label='+1σ')\n",
    "    ax3.axhline(vol_mean - vol_std, color='orange', linestyle=':', alpha=0.7, label='-1σ')\n",
    "    \n",
    "    ax3.set_title('20-Day Rolling Volatility', fontsize=14, weight='bold')\n",
    "    ax3.set_ylabel('Volatility (%)', fontsize=12)\n",
    "    ax3.legend(fontsize=10)\n",
    "    ax3.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax3, data_range_days)\n",
    "\n",
    "# 4. Contango/Backwardation analysis\n",
    "ax4 = fig.add_subplot(gs[2, :])\n",
    "if 'M1_M3_Spread' in ts_metrics.columns:\n",
    "    spread_data = ts_metrics['M1_M3_Spread'].dropna()\n",
    "    \n",
    "    # Create color-filled areas\n",
    "    contango_mask = spread_data > 0\n",
    "    backwardation_mask = spread_data < 0\n",
    "    \n",
    "    ax4.fill_between(spread_data.index, 0, spread_data, \n",
    "                    where=contango_mask, color='green', alpha=0.3, \n",
    "                    label='Contango', interpolate=True)\n",
    "    ax4.fill_between(spread_data.index, 0, spread_data, \n",
    "                    where=backwardation_mask, color='red', alpha=0.3, \n",
    "                    label='Backwardation', interpolate=True)\n",
    "    \n",
    "    ax4.plot(spread_data.index, spread_data, \n",
    "            color='black', linewidth=2, alpha=0.8)\n",
    "    \n",
    "    # Calculate statistics\n",
    "    total_days = len(spread_data)\n",
    "    contango_pct = (contango_mask.sum() / total_days) * 100\n",
    "    backwardation_pct = (backwardation_mask.sum() / total_days) * 100\n",
    "    \n",
    "    # Add statistics text\n",
    "    ax4.text(0.02, 0.95, f'Contango: {contango_pct:.1f}% of time', \n",
    "            transform=ax4.transAxes, fontsize=11, \n",
    "            bbox=dict(boxstyle='round', facecolor='lightgreen', alpha=0.8))\n",
    "    ax4.text(0.02, 0.85, f'Backwardation: {backwardation_pct:.1f}% of time', \n",
    "            transform=ax4.transAxes, fontsize=11,\n",
    "            bbox=dict(boxstyle='round', facecolor='lightcoral', alpha=0.8))\n",
    "\n",
    "    ax4.axhline(y=0, color='black', linestyle='-', alpha=0.8)\n",
    "    ax4.set_title('Market Structure: Contango vs Backwardation (M1-M3)', fontsize=16, weight='bold')\n",
    "    ax4.set_ylabel('M1-M3 Spread (USD/tonne)', fontsize=12)\n",
    "    ax4.legend(fontsize=11)\n",
    "    ax4.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax4, data_range_days)\n",
    "\n",
    "plt.suptitle('Copper Futures Term Structure Analysis', fontsize=18, weight='bold', y=0.98)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Summary statistics\n",
    "print(\"\\nTerm Structure Summary:\")\n",
    "if not ts_metrics.empty:\n",
    "    latest_metrics = ts_metrics.dropna().iloc[-1]\n",
    "    print(f\"Latest M1-M3 spread: ${latest_metrics.get('M1_M3_Spread', 'N/A'):.2f}/tonne\")\n",
    "    print(f\"Current market structure: {'Contango' if latest_metrics.get('M1_M3_Spread', 0) > 0 else 'Backwardation'}\")"
   ]
  },
