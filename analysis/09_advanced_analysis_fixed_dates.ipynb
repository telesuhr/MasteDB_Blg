   "source": [
    "# Advanced liquidity analysis\n",
    "def calculate_liquidity_metrics(volume_data, oi_data):\n",
    "    \"\"\"Calculate liquidity metrics\"\"\"\n",
    "    metrics = pd.DataFrame(index=volume_data.index)\n",
    "    \n",
    "    # Total metrics\n",
    "    metrics['Total_Volume'] = volume_data.sum(axis=1, skipna=True)\n",
    "    metrics['Total_OI'] = oi_data.sum(axis=1, skipna=True)\n",
    "    \n",
    "    # Concentration (top 3 contracts)\n",
    "    metrics['Volume_Concentration'] = volume_data.apply(\n",
    "        lambda x: (x.nlargest(3).sum() / x.sum()) if x.sum() > 0 else np.nan, axis=1\n",
    "    )\n",
    "    \n",
    "    # Turnover ratio\n",
    "    metrics['Turnover_Ratio'] = metrics['Total_Volume'] / metrics['Total_OI']\n",
    "    metrics['Turnover_Ratio'] = metrics['Turnover_Ratio'].replace([np.inf, -np.inf], np.nan)\n",
    "    \n",
    "    return metrics.dropna(how='all')\n",
    "\n",
    "# Calculate liquidity metrics\n",
    "liquidity_metrics = calculate_liquidity_metrics(volume_pivot, oi_pivot)\n",
    "\n",
    "# Create liquidity visualization\n",
    "fig = plt.figure(figsize=(18, 10))\n",
    "gs = GridSpec(2, 3, figure=fig, hspace=0.3, wspace=0.3)\n",
    "\n",
    "# 1. Total volume over time\n",
    "ax1 = fig.add_subplot(gs[0, :])\n",
    "if 'Total_Volume' in liquidity_metrics.columns:\n",
    "    total_vol = liquidity_metrics['Total_Volume'].dropna()\n",
    "    \n",
    "    # Plot volume with trend\n",
    "    ax1.fill_between(total_vol.index, 0, total_vol, alpha=0.3, color=COLORS['primary'])\n",
    "    ax1.plot(total_vol.index, total_vol, color=COLORS['primary'], linewidth=2)\n",
    "    \n",
    "    # Add moving average\n",
    "    vol_ma = total_vol.rolling(20).mean()\n",
    "    ax1.plot(vol_ma.index, vol_ma, color=COLORS['secondary'], \n",
    "            linewidth=2, linestyle='--', label='20-day MA')\n",
    "    \n",
    "    ax1.set_title('Total Daily Volume', fontsize=16, weight='bold')\n",
    "    ax1.set_ylabel('Volume (contracts)', fontsize=12)\n",
    "    ax1.legend(fontsize=11)\n",
    "    ax1.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax1, data_range_days)\n",
    "\n",
    "# 2. Volume concentration\n",
    "ax2 = fig.add_subplot(gs[1, 0])\n",
    "if 'Volume_Concentration' in liquidity_metrics.columns:\n",
    "    conc_data = liquidity_metrics['Volume_Concentration'].dropna() * 100\n",
    "    \n",
    "    ax2.plot(conc_data.index, conc_data, \n",
    "            color=COLORS['tertiary'], linewidth=2)\n",
    "    \n",
    "    # Add average line\n",
    "    conc_mean = conc_data.mean()\n",
    "    ax2.axhline(conc_mean, color='red', linestyle='--', alpha=0.7, \n",
    "               label=f'Average: {conc_mean:.1f}%')\n",
    "    \n",
    "    ax2.set_title('Volume Concentration\\n(Top 3 Contracts)', fontsize=14, weight='bold')\n",
    "    ax2.set_ylabel('Concentration (%)', fontsize=12)\n",
    "    ax2.legend(fontsize=10)\n",
    "    ax2.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax2, data_range_days)\n",
    "\n",
    "# 3. Turnover ratio\n",
    "ax3 = fig.add_subplot(gs[1, 1])\n",
    "if 'Turnover_Ratio' in liquidity_metrics.columns:\n",
    "    turnover_clean = liquidity_metrics['Turnover_Ratio'].dropna()\n",
    "    \n",
    "    # Remove extreme outliers for better visualization\n",
    "    q99 = turnover_clean.quantile(0.99)\n",
    "    turnover_clean = turnover_clean[turnover_clean <= q99]\n",
    "    \n",
    "    ax3.plot(turnover_clean.index, turnover_clean, \n",
    "            color=COLORS['quaternary'], linewidth=2)\n",
    "    \n",
    "    # Add trend line\n",
    "    if len(turnover_clean) > 10:\n",
    "        z = np.polyfit(range(len(turnover_clean)), turnover_clean.values, 1)\n",
    "        p = np.poly1d(z)\n",
    "        ax3.plot(turnover_clean.index, p(range(len(turnover_clean))), \n",
    "                \"--\", color='red', alpha=0.8, linewidth=2, label='Trend')\n",
    "    \n",
    "    ax3.set_title('Volume/OI Turnover Ratio', fontsize=14, weight='bold')\n",
    "    ax3.set_ylabel('Turnover Ratio', fontsize=12)\n",
    "    ax3.legend(fontsize=10)\n",
    "    ax3.grid(True, alpha=0.3)\n",
    "    \n",
    "    format_date_axis(ax3, data_range_days)\n",
    "\n",
    "# 4. Current market structure\n",
    "ax4 = fig.add_subplot(gs[1, 2])\n",
    "latest_date = df['TradeDate'].max()\n",
    "latest_market = df[df['TradeDate'] == latest_date]\n",
    "\n",
    "if not latest_market.empty:\n",
    "    # Group by tenor and sum volume/OI\n",
    "    tenor_summary = latest_market.groupby('TenorNumber').agg({\n",
    "        'Volume': 'sum',\n",
    "        'OpenInterest': 'sum'\n",
    "    }).fillna(0)\n",
    "    \n",
    "    tenors = sorted(tenor_summary.index)\n",
    "    volume_data = tenor_summary.loc[tenors, 'Volume']\n",
    "    oi_data = tenor_summary.loc[tenors, 'OpenInterest']\n",
    "    \n",
    "    # Create dual-axis bar chart\n",
    "    x_pos = np.arange(len(tenors))\n",
    "    width = 0.35\n",
    "    \n",
