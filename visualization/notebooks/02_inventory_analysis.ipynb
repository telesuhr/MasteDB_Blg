   "source": [
    "## 4. 在庫と価格の関係性分析"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 在庫と価格データの統合\n",
    "# LME価格とLME在庫の関係性\n",
    "lme_analysis = pd.DataFrame()\n",
    "\n",
    "if 'LME' in inventory_pivot.columns:\n",
    "    # LME在庫データ\n",
    "    lme_analysis['Date'] = inventory_pivot.index\n",
    "    lme_analysis['LME_Inventory'] = inventory_pivot['LME'].values\n",
    "    \n",
    "    # LME価格データ\n",
    "    lme_prices = cash_prices[cash_prices['ExchangeCode'] == 'LME']\n",
    "    lme_price_pivot = lme_prices.set_index('TradeDate')['LastPrice']\n",
    "    \n",
    "    # 日付でマージ\n",
    "    lme_analysis = lme_analysis.set_index('Date')\n",
    "    lme_analysis = lme_analysis.join(lme_price_pivot.rename('LME_Price'), how='inner')\n",
    "    lme_analysis = lme_analysis.dropna()\n",
    "    \n",
    "    print(f\"📊 LME在庫・価格分析データ: {len(lme_analysis)} レコード\")\n",
    "    \n",
    "    # 相関分析\n",
    "    correlation = lme_analysis['LME_Inventory'].corr(lme_analysis['LME_Price'])\n",
    "    print(f\"🔗 LME在庫・価格相関係数: {correlation:.3f}\")\n",
    "    \n",
    "    lme_analysis.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 在庫と価格の関係性可視化\n",
    "if not lme_analysis.empty:\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=2,\n",
    "        subplot_titles=('LME在庫・価格推移', '在庫 vs 価格散布図', '在庫・価格正規化推移', '相関分析（移動相関）'),\n",
    "        specs=[[{\"secondary_y\": True}, {\"secondary_y\": False}],\n",
    "               [{\"secondary_y\": False}, {\"secondary_y\": False}]],\n",
    "        vertical_spacing=0.1\n",
    "    )\n",
    "    \n",
    "    # 1. 在庫・価格推移（二軸グラフ）\n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=lme_analysis.index,\n",
    "            y=lme_analysis['LME_Inventory'],\n",
    "            mode='lines',\n",
    "            name='LME在庫',\n",
    "            line=dict(color='blue', width=2),\n",
    "            yaxis='y'\n",
    "        ),\n",
    "        row=1, col=1\n",
    "    )\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=lme_analysis.index,\n",
    "            y=lme_analysis['LME_Price'],\n",
    "            mode='lines',\n",
    "            name='LME価格',\n",
    "            line=dict(color='red', width=2),\n",
    "            yaxis='y2'\n",
    "        ),\n",
    "        row=1, col=1\n",
    "    )\n",
    "    \n",
    "    # 2. 散布図\n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=lme_analysis['LME_Inventory'],\n",
    "            y=lme_analysis['LME_Price'],\n",
    "            mode='markers',\n",
    "            name='在庫 vs 価格',\n",
    "            marker=dict(color='green', size=5, opacity=0.6),\n",
    "            hovertemplate='在庫: %{x:,.0f} トン<br>' +\n",
    "                         '価格: %{y:,.0f} USD/MT<extra></extra>'\n",
    "        ),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # 回帰線追加\n",
    "    z = np.polyfit(lme_analysis['LME_Inventory'], lme_analysis['LME_Price'], 1)\n",
    "    p = np.poly1d(z)\n",
    "    x_reg = np.linspace(lme_analysis['LME_Inventory'].min(), lme_analysis['LME_Inventory'].max(), 100)\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=x_reg,\n",
    "            y=p(x_reg),\n",
    "            mode='lines',\n",
    "            name=f'回帰線 (r={correlation:.3f})',\n",
    "            line=dict(color='red', dash='dash')\n",
    "        ),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # 3. 正規化推移（0-1スケール）\n",
    "    from sklearn.preprocessing import MinMaxScaler\n",
    "    scaler = MinMaxScaler()\n",
    "    normalized_data = scaler.fit_transform(lme_analysis[['LME_Inventory', 'LME_Price']])\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=lme_analysis.index,\n",
    "            y=normalized_data[:, 0],\n",
    "            mode='lines',\n",
    "            name='在庫（正規化）',\n",
    "            line=dict(color='blue', width=2)\n",
    "        ),\n",
    "        row=2, col=1\n",
    "    )\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=lme_analysis.index,\n",
    "            y=normalized_data[:, 1],\n",
    "            mode='lines',\n",
    "            name='価格（正規化）',\n",
    "            line=dict(color='red', width=2)\n",
    "        ),\n",
    "        row=2, col=1\n",
    "    )\n",
    "    \n",
    "    # 4. 移動相関（30日）\n",
    "    rolling_corr = lme_analysis['LME_Inventory'].rolling(window=30).corr(lme_analysis['LME_Price'])\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(\n",
    "            x=rolling_corr.index,\n",
    "            y=rolling_corr,\n",
    "            mode='lines',\n",
    "            name='30日移動相関',\n",
    "            line=dict(color='purple', width=2)\n",
    "        ),\n",
    "        row=2, col=2\n",
    "    )\n",
    "    \n",
    "    # ゼロライン追加\n",
    "    fig.add_hline(y=0, line_dash=\"dash\", line_color=\"black\", opacity=0.5, row=2, col=2)\n",
    "    \n",
    "    # レイアウト設定\n",
    "    fig.update_layout(\n",
    "        height=800,\n",
    "        title_text=\"🔸 在庫・価格関係性分析\",\n",
    "        title_x=0.5\n",
    "    )\n",
    "    \n",
    "    # 軸設定\n",
    "    fig.update_xaxes(title_text=\"日付\", row=1, col=1)\n",
    "    fig.update_xaxes(title_text=\"在庫量 (トン)\", row=1, col=2)\n",
    "    fig.update_xaxes(title_text=\"日付\", row=2, col=1)\n",
    "    fig.update_xaxes(title_text=\"日付\", row=2, col=2)\n",
    "    \n",
    "    fig.update_yaxes(title_text=\"在庫量 (トン)\", secondary_y=False, row=1, col=1)\n",
    "    fig.update_yaxes(title_text=\"価格 (USD/MT)\", secondary_y=True, row=1, col=1)\n",
    "    fig.update_yaxes(title_text=\"価格 (USD/MT)\", row=1, col=2)\n",
    "    fig.update_yaxes(title_text=\"正規化値 (0-1)\", row=2, col=1)\n",
    "    fig.update_yaxes(title_text=\"相関係数\", row=2, col=2)\n",
    "    \n",
    "    fig.show()"
   ]
  },
